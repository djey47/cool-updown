{
  "name": "cool-updown",
  "version": "0.0.2",
  "description": "Script to enable scheduled server start/stop",
  "main": "lib/coolupdown",
  "dependencies": {
    "app-root-dir": "^1.0.2",
    "config": "^1.30.0",
    "cron": "^1.3.0",
    "date-fns": "^1.29.0",
    "express": "^4.16.3",
    "express-basic-auth": "^1.1.5",
    "lodash": "^4.17.10",
    "node-ssh": "^5.1.1",
    "shelljs": "^0.8.2",
    "wake_on_lan": "^1.0.0",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.12.0",
    "flow-bin": "^0.73.0",
    "jest": "^22.4.4"
  },
  "scripts": {
    "start": "npm run build && node lib/coolupdown.js",
    "start:service": "nohup npm start >/dev/null 2>&1 &",
    "start:service-debug": "mkdir -p ./logs && nohup npm start >>./logs/debug.log 2>&1 &",
    "stop": "pkill --signal SIGINT cool-updown",
    "logs": "tail -500f logs/app.log",
    "logs:all": "less logs/app.log",
    "lint": "eslint ./src && flow",
    "flow:defs": "flow-typed install -f 0.73 --overwrite",
    "test": "export NODE_ENV=test && jest",
    "jest:watch": "export NODE_ENV=test && jest --watch src",
    "build": "babel src/ -d lib/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djey47/cool-updown.git"
  },
  "keywords": [
    "nodejs",
    "updown",
    "cron",
    "wol",
    "ssh"
  ],
  "author": "djey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djey47/cool-updown/issues"
  },
  "homepage": "https://github.com/djey47/cool-updown#readme",
  "jest": {
    "setupFiles": []
  }
}
